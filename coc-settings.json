{
  "suggest.enablePreview": true,
  "suggest.snippetIndicator": " ",
  "suggest.maxCompleteItemCount": 20,
  "suggest.echodocSupport": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯦ",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "vue": ["javascript", "html"]
  },
  "signature.preferShownAbove": true,
  "signature.target": "echo",
  "html.filetypes": ["html", "vue"],
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.alwaysShowStatus": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToBufferVar": true,
  "explorer.file.column.indent.indentLine": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": "labeling",
  "explorer.quitOnOpen": true,
  "explorer.width": 30,
  "git.enableGutters": false,
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "intelephense.licenceKey": "00BR6YDLX9HRNT4",
  "phpls.path": "/usr/local/bin/intelephense",
  "css.customData": ["./css_custom_data.json"],
  "css.lint.unknownAtRules": "ignore",
  "scss.validate": false
}
