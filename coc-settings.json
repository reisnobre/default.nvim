{
  "suggest.enablePreview": true,
  "suggest.snippetIndicator": " ",
  "suggest.maxCompleteItemCount": 20,
  "suggest.echodocSupport": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯦ",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "vue": ["javascript", "html"]
  },
  "signature.preferShownAbove": true,
  "signature.target": "echo",
  "html.filetypes": ["html", "vue"],
  "eslint.packageManager": "yarn",
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "vue"],
  "git.addGBlameToVirtualText" : true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "explorer.file.column.indent.indentLine": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": true,
  "explorer.previewAction.strategy": "labeling",
  "explorer.quitOnOpen": true,
  "explorer.width": 30,
  "git.enableGutters": false,
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true
}
